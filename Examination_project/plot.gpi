set terminal svg 
	set output 'plot.svg' 
	set key top right 
	set title 'Bi linear splines of F' 
	set xlabel 'x' 
	set ylabel 'y' 
	set zlabel 'z'
	set zrange [0.8:1.0]
	set xzeroaxis 
	set yzeroaxis 
	set zzeroaxis 
	set samples 1600 
	set dgrid3d 10,10 
	set hidden3d 
	splot 'plot.data' using ($1):($2):($3) index 0 with lines title 'Linearly Interpulated points' 	,1/(x**2+y**2+1.0) with lines title 'The chosen function' 	, 'plot.data' using ($1):($2):($3) index 0 with lines title 'Graph' 	
